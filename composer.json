{
    "name": "atmorus/laravel-clear-all-cached-data",
    "description": "Laravel 5 Package to Clear all cached data i.e. views, config, routes etc. !!! Important !!!  The basis of this package was taken code Afraz Ahmad - github.com/afrazahmmad/clear-all-cached-data. Changes were made to the properties of the link to the package: requires (JSON schema), to support Laravel 5.7",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "homepage": "https://github.com/atmorus/laravel-clear-all-cached-data",
	"keywords": [
        "atmorus",
        "laravel-clear-all-cached-data"
    ],
    "authors": [
        {
            "name": "Afraz Ahmad",
            "email": "afrazahmad101.ca@gmail.com"
        },
        {
            "name": "MORUS",
            "email": "rmorozov.gm@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "AtMorus\\ClearAllCachedData\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AtMorus\\ClearAllCachedData\\ClearAllCachedDataProvider"
            ]
        }
    }
}
